{
  "name": "next-ts-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:client dev:server",
    "dev:client": "next dev",
    "dev:server": "npm run dev --prefix server",
    "dev:proxy": "next dev & local-ssl-proxy --key localhost-key.pem --cert localhost.pem --source 3001 --target 3000",
    "build": "next build && next export",
    "lint": "run-p lint:js lint:prettier lint:style",
    "lint:js": "eslint --ext .ts,.tsx,.js --ignore-path .gitignore .",
    "lint:prettier": "prettier --check \"./**/*.{ts,tsx,js}\" --ignore-path .gitignore",
    "lint:style": "stylelint \"**/*.ts{,x}\"",
    "lint:fix": "npm run lint:js -- --fix && prettier --write \"./**/*.{ts,tsx,js}\" --ignore-path .gitignore && npm run lint:style -- --fix",
    "start": "run-p start:client start:server",
    "start:client": "next start -p 8080",
    "start:server": "npm start --prefix server",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.4.1",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.9",
    "@mui/material": "^5.10.7",
    "@mui/styled-engine-sc": "^5.10.6",
    "@zxing/browser": "^0.1.1",
    "@zxing/library": "^0.19.1",
    "discord.js": "^14.7.1",
    "dotenv": "^16.0.3",
    "framer-motion": "^7.6.12",
    "next": "^12.2.5",
    "qrcode": "^1.5.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-hook-form": "^7.39.5",
    "react-modal": "^3.16.1",
    "react-qr-reader": "^3.0.0-beta-1",
    "react-zxing": "^1.0.5",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@types/node": "^18.11.18",
    "@types/qrcode": "^1.5.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/react-modal": "^3.13.1",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "babel-plugin-styled-components": "^2.0.7",
    "eslint": "^8.17.0",
    "eslint-config-next": "^12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "local-ssl-proxy": "^1.3.0",
    "npm-run-all": "^4.1.5",
    "postcss-jsx": "^0.36.4",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.6.2",
    "prettier-plugin-organize-imports": "^2.3.4",
    "stylelint": "^14.8.5",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4"
  }
}
